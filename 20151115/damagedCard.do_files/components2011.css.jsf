/*TRANSIT MENU*/
#transitMenu {
	margin: 0px;
	width: 100px;
	padding: 5px;
	background: #e6e6e6;
	float: left;
	overflow: hidden;
}

#transitMenu li {
	padding: 5px;
	width: 90px;
	background-image: #e6e6e6;
}

#transitMenu li:hover {
	background-color: #8c8c8c;
	color: #FFF;
}

#transitMenu li a {
	display: block;
	text-decoration:none;
	color: #333333;
}


#transitMenu li .transitButton {
	display: block;
	background-color: transparent;
	cursor: pointer;
	color: #505050;
}

#mainContent .transitContent {
	text-align: left;
	width: 234px;
	float: left;
}

#mainContent #transitBox {
	background:#FFF;
	width:236px;
	border-left: 1px solid #dbdcde;
	padding:5px;
	text-align:left;
	float: left;
}

#mainContent #transitBox .productBox {
	width:214px;
	/* [disabled]height:160px; */
	overflow:auto;
	background:#e6e6e6;
	padding:10px;
	min-height: 160px;
}

#mainContent #transitBox .productBox .radioDiv {
	margin-left: 0;
	margin-bottom: 5px;
	width: 200px;
}

#mainContent #transitBox .productBox .radioDiv .radioQuestion {
	margin-left: 5px;
	width: 170px;
}

#mainContent #transitBox .productBox .radioDiv .textQuestion {
	width: 80px;	
}

#mainContent #transitBox .productBox .radioDiv .textAnswer {
	width: 80px;	
}

#mainContent .transitImage {
	float: left;
	margin: 0 4px 5px 0;
	display:block;
	cursor: pointer;
}
#mainContent .transitImage img{
	border:none;
	outline:none;
}
input[type='radio']{
	border:none;
	outline:none;
	background-color:transparent;
}

.cardID {
	width: 290px;
	position: relative;
	overflow: hidden;
}
	
.plus {
	float:right;
	width:10px;
	height:10px;
	cursor: pointer;
	position: relative;
}

.loginLine{
	float:left;
	clear:left;
	margin-bottom:3px;
}
.loginLine div{
	font-size:.75em;
	width:140px;
	float:left;
}
.loginLine .valMsg{
	width:10px;
	margin-top:-2px;
}
