/*********************************************************
FOR ROLLOVER MYCLIPPER
*********************************************************/
#myRollover1, #myRollover2 {
	width: 160px;
	position: absolute;
	top: 66px;
	right: 0px;
	cursor:pointer;
}

h1#mytranslinkModuleRollover,
h1#myclipperModuleRollover  {
	height: 34px;
	background: #fff url("/ClipperCard/javax.faces.resource/login.jpg.jsf?ln=images/global") no-repeat;
	color: #FFFFFF;
	text-indent: -9999px;
	
}

h1#myclipperModuleRolloverLoggedIn  {
	height: 34px;
	background: #fff url("/ClipperCard/javax.faces.resource/myClipper.jpg.jsf?ln=images/global") no-repeat;
	color: #FFFFFF;
	text-indent: -9999px;
	
}

h1#myclipperLoggedIn  {
	height: 34px;
	background: #fff url("/ClipperCard/javax.faces.resource/myClipper_active.jpg.jsf?ln=images/global") no-repeat;
	color: #FFFFFF;
	text-indent: -9999px;
	
}

