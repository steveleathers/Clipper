@charset "utf-8";
/* CSS Document */

.prop500 {
	height:500px;
	float:right;
	width:1px;
}
.prop200 {
	height:250px;
	float:right;
	width:1px;
}

body#myClipperPage #mainContent {
	width: 366px;
	float:left;
	margin: 0 0 0 7px;
}

body#myClipperPage #mainContent.twocolumn {
	width: 590px;
}

#rightCol216 {
	margin: 35px 0 4px 9px;
	float:left;
	width: 234px;
}

li.navSelected {
	background: #0068a6 url("/ClipperCard/javax.faces.resource/leftnav-color-button.gif.jsf?ln=images/global") repeat-x top;
	border-top: 1px solid #8ebdd8;
}

/*PROCESS STEPS*/
#mainContent .contentSubHeader {
	height: 35px;
}

#mainContent .stepHeader {
	height: 21px;
	background: #1c95e4 url("/ClipperCard/javax.faces.resource/stepHeader.gif.jsf?ln=images/global") repeat-x;
	line-height: 1em;
	border-bottom: 2px solid #012f6d;
	width: 365px;
}

#mainContent .stepHeader div.title {
	float:left;
	position:absolute;
	width:200px;
	height:21px;
	overflow:hidden;
	color: #FFF;
}

#mainContent .stepHeader div.policy {
	float:right;
	position:relative;
	width:90px;
	height:21px;
	overflow:hidden;
}

#mainContent #navBar {
	height: 21px;
	line-height: 1em;
	border-bottom: 2px solid #012f6d;
	width: 365px;
}

#mainContent #navBar ul#navTabs {
	overflow: hidden;
	list-style-type: none;
}

#mainContent #navBar ul#navTabs li {
	float: left;
	margin: 0 2px 0 0;
	height: 21px;
	width: 85px;
}

#mainContent #navBar ul#navTabs li .textLink a {
	display: block;
	height:21px;
	text-decoration: none;
	color: #FFF;
	background-color: #86bde0;
	padding-top: 2px;
}

#mainContent #navBar ul#navTabs li a.selected {
	background-image: url("/ClipperCard/javax.faces.resource/stepHeader.gif.jsf?ln=images/global");
	height: 21px;
	background-repeat: repeat-x;
}

#mainContent .greyBox {
	border: 1px solid #dbdcde;
	border-top: none;
	padding: 10px 10px 10px 20px;
	width: 333px;
	overflow: hidden;
}

#mainContent .greyBox2 {
	border: 1px solid #dbdcde;
	border-top: none;
	padding: 10px 2px 10px 2px;
	width: 359px;
}

#mainContent .topBorder {
	border: 1px solid #dbdcde;
}

#mainContent .greyBox p {
	margin: 0 0 10px 0;
	padding: 0;
	width: 330px;
}

#mainContent .greyBox2 p {
	margin: 0 0 10px 18px;
	padding: 0;
	width: 330px;
	color: #333333;
}

#mainContent .greyBox2 .radioQuestion p{
	margin-left: 0;
	width: 290px;
}

.helpContainer .helpTextMargin {
	margin: 10px;
}

#mainContent .helpTextMargin p {
	width: 204px;
	padding: 0;
}

#mainContent hr {
	width: 323px;
	height: 1px;
	background-color: #999999;
	color: #999999;
	margin: 20px 0 20px 0;
	border: none;
}

#mainContent .greyBox .radioQuestion p {
	width: 290px;
}

#mainContent .greyBox .radioQuestion label {
	width: 290px;
	color: inherit;
	font-size: inherit;
}

.greyInside {
	background-color:#e6e6e6;
}

.spacer {height: 5px;}
.spacer10 {height: 10px;}

#mainContent .greyBox .radioDiv,
#mainContent .subDiv {
	position: relative;
	width: 343px;
	overflow: hidden;
}

#mainContent .greyBox2 .radioDiv {
	position: relative;
	width: 323px;
	overflow: hidden;
	margin-left:20px;
}

#mainContent .infoDiv {
	position: relative;
	width: 333px;
	overflow: hidden;	
}

#mainContent .infoDiv2 {
	position: relative;
	width: 353px;
	overflow: hidden;	
}

#mainContent .subDiv .linkDiv {
	width: 318px;
	text-align: right;
	height: 20px;
}

radioDiv input {
	z-index: 8999;
}

/*Form Questions*/
.radioBtn {
	width:20px;
	float:left;
	position:relative;
}

.radioQuestion {
	width:300px;
	float:left;
	position:relative;
	margin-Left:10px;
}

.cardName {
	width:140px;
	float:left;
	position:relative;
	margin-Left:10px;
}

.greyBox2 .radioQuestion {
	width:290px;
}

.radioDiv .textQuestion {
	width:150px;
	float:left;
	position:relative;
	margin-top: 3px;
	height: 20px;
}

.subDiv .textQuestion {
	width:140px;
	float:left;
	position:relative;
	overflow: visible;
	margin-top: 3px;
	height: 20px;
	margin-left: 30px;
}

/*ACCOUNT INFO*/
.fieldName {
	width:150px;
	float:left;
	position:relative;
	margin-top: 3px;
}

.fieldData {
	width:183px;
