	margin-top: 3px;
}

.field90  {width:90px;}
.field100  {width:100px;}
.field200  {
	width:200px;
}


.helpToggleInactive{
	background-image: url("/ClipperCard/javax.faces.resource/QuestionMark_FullOpacity.png.jsf?ln=images/buttons");
}

.helpToggleActive{
	background-image: url("/ClipperCard/javax.faces.resource/QuestionMark_LowOpacity.png.jsf?ln=images/buttons");
}

.helpToggle {
	width: 21px;
	height:18px;
	top:-2px;
	z-index: 80;
	cursor:pointer;
}



