.progress_bar	{
	margin:10px 0 15px 0;
	list-style:none;
}

.progress_bar li	{
	height:28px;
	padding:7px 5px 5px 32px;
	display: inline;
	color:#535353;
	font-size:12px;
}

.progress_bar li.stepone	{
	background-image: url("/ClipperCard/javax.faces.resource/one.gif.jsf?ln=images/bg");
	background-repeat: no-repeat;
	background-position: 0 0;
}

.progress_bar li.steptwo	{
	background-image: url("/ClipperCard/javax.faces.resource/two.gif.jsf?ln=images/bg");
	background-repeat: no-repeat;
	background-position: 0 0;
}

.progress_bar li.stepthree	{
	background-image: url("/ClipperCard/javax.faces.resource/three.gif.jsf?ln=images/bg");
	background-repeat: no-repeat;
	background-position: 0 0;
}

.progress_bar li.stepfour	{
	background-image: url("/ClipperCard/javax.faces.resource/four.gif.jsf?ln=images/bg");
	background-repeat: no-repeat;
	background-position: 0 0;
}

.progress_bar li.stepfive	{
	background-image: url("/ClipperCard/javax.faces.resource/five.gif.jsf?ln=images/bg");
	background-repeat: no-repeat;
	background-position: 0 0;
}

.progress_bar li.stepsix	{
	background-image: url("/ClipperCard/javax.faces.resource/six.gif.jsf?ln=images/bg");
	background-repeat: no-repeat;
	background-position: 0 0;
}

.progress_bar li.on	{
	color:#002e6e;
	font-weight:bold;
	background-position: 0 -27px;
}

.progress_bar li a	{
	color:#0068a6;
	text-decoration:none;
	font-weight:normal;
}

.progress_bar li a:hover	{
	color:#0068a6;
	text-decoration:none;
}

.progress_bar li a:visited	{
	color:#0068a6;
}

